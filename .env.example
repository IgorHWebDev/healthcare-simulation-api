# API Configuration
PORT=8000
ENVIRONMENT=development
DEBUG=true

# Security
# Get your Render API key from: https://dashboard.render.com/u/settings#api-keys
RENDER_API_KEY=your_render_api_key_here
API_KEY_HEADER=X-RapidAPI-Key
DEFAULT_API_KEY=your_rapidapi_key_here

# Database Configuration
DB_USER=healthcare
DB_PASSWORD=your_secure_password_here
DB_NAME=healthcare_db
DB_HOST=localhost
DB_PORT=5433

# M3 Optimization Settings
M3_OPTIMIZER_ENABLED=true
METAL_FRAMEWORK_ENABLED=true
BATCH_SIZE=256
MAX_PARALLEL_CIRCUITS=1000

# Redis Cache Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here

# CORS
CORS_ORIGINS=["*"]

# Domain Configuration
DOMAIN=api.iqhis.dev
STAGING_DOMAIN=staging.api.iqhis.dev

# Service Configuration
PYTHON_VERSION=3.11.0